{{#power-select
    search=(action "search")
    onchange=(action "openSelected")
    placeholder="Search"
    onopen=(action "onFocus")
    onclose=(action "onBlur")
    searchEnabled=false
    triggerComponent="gh-search-input/trigger"
    renderInPlace=true
    loadingMessage="Loading"
    as |name term|}}
    {{highlighted-text name.title term}}
{{/power-select}}
